<p id="notice"><%= notice %></p>

<p>
  <strong>User:</strong>
  <%= @time_entry.user_id %>
</p>

<p>
  <strong>Time in:</strong>
  <%= @time_entry.starts_at.try(:to_s, :hours_mins) %>
</p>

<%if @time_entry.finish_at.present?%>
	<p>
	  <strong>Time out:</strong>
	  <%= @time_entry.finish_at.try(:to_s, :hours_mins) %>
	</p>
<% end %>

<%= link_to 'Edit', edit_time_entry_path(@time_entry) %> |
<%= link_to 'Back', time_entries_path %>
